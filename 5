#include <stdio.h>
#include <limits.h>
#define N 5
#define M 5

main() {
int a[N][M], i, j,c,s;
c=0;
    for (i=0; i<N; i++) {
    for (j=0; j<M; j++) {
    a[i][j] = rand() % 100;
    printf(" %d ", a[i][j]);
}
printf("\n");
}
    for (j=0; j<M; j++) {
    for (i=1; i<=N; i++){

    if (a[i][j] %2!=0)
    c=c+1;
}

if(c==2)
s=j;

c=0;
}
printf("%d",s);
return (0);
}
