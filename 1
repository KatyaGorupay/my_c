#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define M 5 /* Число строк матрицы */
#define N 10 /* Число столбцов матрицы */
#include <limits.h>

int main()
{
int a[M][N];
int s [N];
int i, j, min;
min=INT_MAX;

for(i = 0; i < M; i++)
for(j = 0; j < N; j++)
a[i][j] = rand()%100;

for(i = 0; i < M; i++){
for(j = 0; j < N; j++);
{

if (a[j][i]<min)
{
min==a[i][j];
s[i][j]== min;
}
}

}
for(i = 0; i < M; i++){
for(j = 0; j < N; j++)
printf("%5d", a[i][j]);
printf("\n");
}

return 0;
printf("minimum= %d\n",s[i][j]);
}
